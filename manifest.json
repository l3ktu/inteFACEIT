{
   "manifest_version": 2,
   "name": "inteFACEIT",
   "version": "1.0.6",
   "update_url": "https://raw.githubusercontent.com/l3ktu/inteFACEIT/main/update.xml",
   "description": "Folosind aceasta extensie iti vei usura viata.",
   "icons": {
      "128": "/img/no_back_logo.png"
    },
   "content_scripts": [
      {
         "js": [
            "content.js"
         ],
         "matches": [
            "https://www.faceit.com/en/clan/*"
         ]
      }
   ],
   "options_page": "popup.html",
   "permissions": [
      "storage"
   ],
   "browser_action": {
      "default_icon": "/img/no_back_logo.png",
      "default_popup": "popup.html"
    }
}
